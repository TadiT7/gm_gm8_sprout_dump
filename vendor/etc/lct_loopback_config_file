tc 1
#Headset-mic to Speaker AFE loopback
!AfeLoop
Txdevice:11
Rxdevice:5

enable
ADC2 Volume:6
DEC1 MUX:ADC2
ADC2 MUX:INP2
IIR1 INP1 MUX:DEC1

RX3 MIX1 INP1:RX1
SPK:Switch

QUIN_MI2S_RX_DL_HL Switch:1
QUIN_MI2S_RX Port Mixer TERT_MI2S_TX:1


disable
QUIN_MI2S_RX Port Mixer TERT_MI2S_TX:0
QUIN_MI2S_RX_DL_HL Switch:0

ADC2 Volume:0
DEC1 MUX:ZERO
ADC2 MUX:ZERO
IIR1 INP1 MUX:ZERO 

RX3 MIX1 INP1:ZERO
SPK:ZERO

